#!/bin/bash

case "$1" in
    PHP_CodeSniffer)
        ./vendor/bin/phpcs
        exit $?
        ;;
    PHPUnit)
        cd ${TESTING_SITE_DIR}
        ./vendor/bin/phpunit -c ./web/core ./web/modules/rdf_entity/tests/src
        exit $?
        ;;
    *)
        echo "Unknown test '$1'"
        exit 1
esac
